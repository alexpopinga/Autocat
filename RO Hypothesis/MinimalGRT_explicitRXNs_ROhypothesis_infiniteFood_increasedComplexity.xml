            </reaction>
            <reaction spec='Reaction' reactionName="Replication30" rate="1">
                	Gcga[c] + Ggcu[c] + Paaa[c] + c[c] + g[c] + a[c] + g[c] + c[c] + u[c] ->  
                	Gcga[c] + Ggcu[c] + Paaa[c] + Gcga[c] + Ggcu[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Replication31" rate="1">
                	Gcgg[c] + Ggcc[c] + Paaa[c] + c[c] + g[c] + g[c] + g[c] + c[c] + c[c] ->  
                	Gcgg[c] + Ggcc[c] + Paaa[c] + Gcgg[c] + Ggcc[c]
            </reaction>

            <!-- Translation reactions, one Class I aaRS and one Class II aaRS -->
            <reaction spec='Reaction' reactionName="Translation" rate="1">
                	Guuu[c] + Gaaa[c] + Paaa[c] + phe[c] + lys[c] ->  
                	Guuu[c] + Gaaa[c] + Paaa[c] + Guuu[c] + Gaaa[c] + Pphe[c] + Plys[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation1" rate="1">
                	Guuc[c] + Gaag[c] + Paaa[c] + phe[c] + lys[c] ->  
                	Guuc[c] + Gaag[c] + Paaa[c] + Guuc[c] + Gaag[c] + Pphe[c] + Plys[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation2" rate="1">
                	Guua[c] + Gaau[c] + Paaa[c] + leu[c] + asn[c] ->  
                	Guua[c] + Gaau[c] + Paaa[c] + Guua[c] + Gaau[c] + Pleu[c] + Pasn[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation3" rate="1">
                	Guug[c] + Gaac[c] + Paaa[c] + leu[c] + asn[c] ->  
                	Guug[c] + Gaac[c] + Paaa[c] + Guug[c] + Gaac[c] + Pleu[c] + Pasn[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation4" rate="1">
                	Gucu[c] + Gaga[c] + Paaa[c] + ser[c] + arg[c] ->  
                	Gucu[c] + Gaga[c] + Paaa[c] + Gucu[c] + Gaga[c] + Pser[c] + Parg[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation5" rate="1">
                	Gucc[c] + Gagg[c] + Paaa[c] + ser[c] + arg[c] ->  
                	Gucc[c] + Gagg[c] + Paaa[c] + Gucc[c] + Gagg[c] + Pser[c] + Parg[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation6" rate="1">
                	Guca[c] + Gagu[c] + Paaa[c] + ser[c] + ser[c] ->  
                	Guca[c] + Gagu[c] + Paaa[c] + Guca[c] + Gagu[c] + Pser[c] + Pser[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation7" rate="1">
                	Gucg[c] + Gagc[c] + Paaa[c] + ser[c] + ser[c] ->  
                	Gucg[c] + Gagc[c] + Paaa[c] + Gucg[c] + Gagc[c] + Pser[c] + Pser[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation8" rate="1">
                	Guau[c] + Gaua[c] + Paaa[c] + tyr[c] + ile[c] ->  
                	Guau[c] + Gaua[c] + Paaa[c] + Guau[c] + Gaua[c] + Ptyr[c] + Pile[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation9" rate="1">
                	Guac[c] + Gaug[c] + Paaa[c] + tyr[c] + met[c] ->  
                	Guac[c] + Gaug[c] + Paaa[c] + Guac[c] + Gaug[c] + Ptyr[c] + Pmet[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation10" rate="1">
                	Guaa[c] + Gauu[c] + Paaa[c] + ile[c] ->  
                	Guaa[c] + Gauu[c] + Paaa[c] + Guaa[c] + Gauu[c] <!--+ STOP--> + Pile[c]
            </reaction>
            <reaction spec='Reaction' reactionName="Translation11" rate="1">
                	Guag[c] + Gauc[c] + Paaa[c] + ile[c] ->  
                	Guag[c] + Gauc[c] + Paaa[c] + Guag[c] + Gauc[c] <!--+ STOP--> + Pile[c]
